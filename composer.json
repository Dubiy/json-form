{
    "name": "mcfedr/json-form",
    "description": "A couple of helper files for handling json with symfony forms",
    "keywords": ["json", "forms", "symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Fred Cox",
            "email": "mcfedr@gmail.com"
        }
    ],
    "type": "library",
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": { "Mcfedr\\JsonForm": "src/" }
    },
    "autoload-dev": {
        "psr-0": { "Mcfedr\\JsonForm": "test/" }
    },
    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": "~2.5",
        "symfony/form": "~2.5"
    },
    "require-dev": {
        "symfony/validator": "~2.5",
        "phpunit/phpunit": "~4.3",
        "theseer/phpdox": "~0.7",
        "phploc/phploc": "~2.0",
        "pdepend/pdepend": "~2.0",
        "phpmd/phpmd": "~2.1",
        "squizlabs/php_codesniffer": "~1.5",
        "sebastian/phpcpd": "~2.0"
    }
}
