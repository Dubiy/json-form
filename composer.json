{
    "name": "mcfedr/json-form",
    "description": "A couple of helper files for handling json with symfony forms",
    "keywords": ["json", "forms", "symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Fred Cox",
            "email": "mcfedr@gmail.com"
        }
    ],
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": { "Mcfedr\\JsonFormBundle\\": "src/Mcfedr/JsonFormBundle" }
    },
    "autoload-dev": {
        "psr-4": { "Mcfedr\\JsonFormBundle\\": "tests/Mcfedr/JsonFormBundle" }
    },
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3",
        "theseer/phpdox": "~0.7",
        "phploc/phploc": "~2.0",
        "pdepend/pdepend": "~2.0",
        "phpmd/phpmd": "~2.1",
        "squizlabs/php_codesniffer": "~1.5",
        "sebastian/phpcpd": "~2.0",
        "symfony/monolog-bundle": "~2.3",
        "sensio/framework-extra-bundle": "~3.0"
    }
}
